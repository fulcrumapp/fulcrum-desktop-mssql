{
  "name": "fulcrum-desktop-mssql",
  "version": "1.0.0",
  "main": "dist/plugin.js",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/babel *.js --out-dir dist --source-maps=inline",
    "watch": "node_modules/.bin/babel *.js --out-dir dist --source-maps=inline --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "fulcrum-desktop-plugin": "^1.4.0",
    "fulcrum-schema": "^2.0.13",
    "mssql": "^4.0.4",
    "snake-case": "^2.1.0",
    "sqldiff": "^0.0.29",
    "wellknown": "^0.5.0"
  },
  "babel": {
    "plugins": [
      "fulcrum-desktop"
    ],
    "presets": [
      "stage-0",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-fulcrum-desktop": "^1.0.0"
  }
}
